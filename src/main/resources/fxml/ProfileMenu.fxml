<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProfileMenuView">
    <stylesheets>
        <URL value="@../CSS/ProfileMenu.css" />
    </stylesheets>
    <children>
        <Label fx:id="username" layoutX="95.0" layoutY="62.0" prefHeight="18.0" prefWidth="306.0" styleClass="label" text="Username : " />
        <Label fx:id="score" layoutX="95.0" layoutY="96.0" prefHeight="18.0" prefWidth="190.0" styleClass="label" text="Score : " />
        <Button layoutX="120.0" layoutY="183.0" mnemonicParsing="false" onAction="#changeUsername" prefHeight="26.0" prefWidth="127.0" text="UsernameChange">
            <font>
                <Font name="Cambria" size="12.0" />
            </font>
        </Button>
        <Button layoutX="120.0" layoutY="230.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="26.0" prefWidth="127.0" text="PasswordChange">
            <font>
                <Font name="Cambria" size="12.0" />
            </font>
        </Button>
        <Label id="title" layoutX="391.0" layoutY="44.0" prefHeight="18.0" prefWidth="166.0" text="Profile Menu">
            <font>
                <Font name="Cambria" size="12.0" />
            </font>
        </Label>
        <Button layoutX="120.0" layoutY="276.0" mnemonicParsing="false" onAction="#deleteAccount" prefHeight="26.0" prefWidth="127.0" text="DeleteAccount">
            <font>
                <Font name="Cambria" size="12.0" />
            </font>
        </Button>
        <Button layoutX="120.0" layoutY="322.0" mnemonicParsing="false" onAction="#logout" prefHeight="26.0" prefWidth="127.0" text="Logout">
            <font>
                <Font name="Cambria" size="12.0" />
            </font>
        </Button>
        <Button layoutX="120.0" layoutY="437.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="26.0" prefWidth="127.0" text="Back To Main">
            <font>
                <Font name="Cambria" size="12.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
